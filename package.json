    "fs-extra": "^4.0.2",
    "glob": "^7.1.3",
    "istanbul": "^0.4.2",
    "jsts": "^1.3.0",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "mocha": "^4.0.0",
    "nock": "^9.0.9",
    "rimraf": "^2.6.1",
    "semantic-release": "^15.12.0",
    "travis-deploy-once": "^5.0.9",
    "yauzl": "^2.6.0",
    "yazl": "^2.4.2"
  },
  "dependencies": {
    "@turf/boolean-point-in-polygon": "^6.0.1",
    "@turf/helpers": "^6.1.3",
